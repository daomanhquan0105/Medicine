<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css", "~/Content/themes/base/css", "~/Content/Font/css/css", "~/Content/Owl.Carousel/css")
    @Styles.Render("~/Content/Slick/css","~/Content/DataTables/css/css", "~/Content/CustomerCss/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)
    
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700;900&display=swap" rel="stylesheet">
</head>
<body>

    @Html.Action("HeaderPartialView", "Home")

    @RenderBody()

    @Html.Action("FooterPartialView", "Home")

    @Scripts.Render("~/bundles/jquery", "~/bundles/jqueryui", "~/bundles/bootstrap")
    @Scripts.Render("~/bundles/Owl.carousel/Script", "~/bundles/Slick/Script")

    <script src="~/Scripts/DataTables/jquery.dataTables.js"></script>
    <script src="~/Scripts/scripts.js"></script>
    <script>
        $("#tablelist").DataTable();
    </script>
    @RenderSection("scripts", required: false)
</body>
</html> 
